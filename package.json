{
  "name": "boilerplate",
  "version": "2.0.0",
  "description": "Boilerplate utilizando Typescript, React, NextJS e Styled Components!",
  "author": "Alquipo Neto",
  "license": "MIT",
  "repository": "github:Alquipo/boilerplate",
  "homepage": "https://www.linkedin.com/in/alquiponeto/",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "generate": "yarn run plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "11.1.0",
    "next-pwa": "^5.3.1",
    "next-seo": "^4.26.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-essentials": "6.3.7",
    "@storybook/builder-webpack5": "^6.3.7",
    "@storybook/manager-webpack5": "^6.3.7",
    "@storybook/react": "6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.6",
    "@types/react": "^17.0.19",
    "@types/styled-components": "^5.1.13",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.0",
    "jest": "^27.1.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "storybook-addon-next-router": "^3.0.7",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1"
  }
}
